{
	"name": "Browser addon (echo msk experiment).",
	"description": "Browser addon (echo msk experiment).",
	"manifest_version": 2,

	"version": "1.0",

	"permissions": 
	[
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"activeTab"
	],

	"browser_action": 
	{
		"default_icon": "icon48.png",
		"default_title": "Browser addon (echo msk experiment)."
	},

	"icons": 
	{
		"48": "icon48.png"
	},

	"background": 
	{
		"scripts": 
		[
			"filters/responcefilter.js", "filters/blockfilter.js", "filters/redirectfilter.js", 
		    "filter_list.js"
		]
	}
}